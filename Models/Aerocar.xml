<!-- AILERONS -->

<animation>
 <type>rotate</type>
 <object-name>aileron_left</object-name>
 <condition>
  <equals>
   <property>controls/doors/transform/position-norm</property>
   <value>1</value>
  </equals>
 </condition>
 <property>fdm/jsbsim/fcs/left-aileron-pos-deg</property>
 <factor>1</factor>
 <axis>
  <x1-m> 0.480</x1-m>
  <y1-m>-5.182</y1-m>
  <z1-m>-0.127</z1-m>
  <x2-m> 0.480</x2-m>
  <y2-m>-3.102</y2-m>
