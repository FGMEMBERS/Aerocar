 <object-name>vstab</object-name>
 <object-name>wing_left</object-name>
 <object-name>wing_right</object-name>
 <property>controls/doors/transform/position-norm</property>
 <interpolation>
  <entry>
   <ind>1.0</ind>
   <dep>0</dep>
  </entry>
  <entry>
   <ind>0.6</ind>
   <dep>0</dep>
  </entry>
  <entry>
   <ind>0.4</ind>
   <dep>4</dep>
  </entry>
  <entry>
   <ind>0.2</ind>
   <dep>4</dep>
  </entry>
  <entry>
   <ind>0.0</ind>
   <dep>2.5</dep>
  </entry>
 </interpolation>
 <axis>
  <x> 1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>aileron_left</object-name>
 <object-name>aileron_right</object-name>
 <object-name>hstab</object-name>
 <object-name>tail</object-name>
 <object-name>vstab</object-name>
