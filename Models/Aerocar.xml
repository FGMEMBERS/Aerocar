<PropertyList>

<path>Aerocar.ac</path>

<nasal>
 <load>
  var livery_update = aircraft.livery_update.new("Aircraft/Aerocar/Models/Liveries");
 </load>
 <unload>
  livery_update.stop();
 </unload>
</nasal> 

<model>
 <path>Aircraft/Aerocar/Models/Panel.xml</path>
 <offsets> 
  <x-m>-1.600</x-m>
  <y-m> 0.000</y-m>
  <z-m>-0.650</z-m>
  <pitch-deg>-15</pitch-deg>
 </offsets>
</model>

<animation> 
 <type>material</type> 
 <object-name>carbody.003</object-name>
 <object-name>door_left</object-name>
 <object-name>door_right</object-name>
 <property-base>sim/model/livery</property-base> 
 <texture-prop>texture</texture-prop> 
 <texture>N103D.png</texture> 
</animation>

<animation>
 <type>shader</type>
 <shader>chrome</shader>
 <texture>glass_shader.png</texture>
 <object-name>door_left_windows</object-name>
 <object-name>door_right_windows</object-name>
 <object-name>front_window</object-name>
</animation>

<!-- DOORS -->

<animation>
 <type>pick</type>
 <object-name>door_left</object-name>
 <object-name>door_left_windows</object-name>
 <action>
  <button>0</button>
  <binding>
   <command>nasal</command>
   <script>aerocar.left_door.toggle();</script>
  </binding>
 </action>
</animation>

<animation>
 <type>rotate</type>
 <object-name>door_left</object-name>
 <object-name>door_left_windows</object-name>
 <property>controls/doors/left/position-norm</property>
 <factor>-70</factor>
 <axis>
  <x1-m>-1.752</x1-m>
  <y1-m>-0.714</y1-m>
  <z1-m>-1.339</z1-m>
  <x2-m>-1.496</x2-m>
  <y2-m>-0.707</y2-m>
  <z2-m>-0.655</z2-m>
 </axis>
</animation>

<animation>
 <type>pick</type>
 <object-name>door_right</object-name>
 <object-name>door_right_windows</object-name>
 <action>
  <button>0</button>
  <binding>
   <command>nasal</command>
   <script>aerocar.right_door.toggle();</script>
  </binding>
 </action>
</animation>

<animation>
 <type>rotate</type>
 <object-name>door_right</object-name>
 <object-name>door_right_windows</object-name>
 <property>controls/doors/right/position-norm</property>
 <factor>70</factor>
 <axis>
  <x1-m>-1.752</x1-m>
  <y1-m> 0.714</y1-m>
  <z1-m>-1.339</z1-m>
  <x2-m>-1.496</x2-m>
  <y2-m> 0.707</y2-m>
  <z2-m>-0.655</z2-m>
 </axis>
</animation>

<!-- GEAR -->

 <animation>
  <type>rotate</type>
  <object-name>gear_front_left</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <factor>-30</factor>
  <center>
   <x-m> -2.277 </x-m>
   <y-m> -1.000 </y-m>
   <z-m> -1.616 </z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>gear_front_right</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <factor>-30</factor>
  <center>
   <x-m> -2.277 </x-m>
   <y-m>  1.000 </y-m>
   <z-m> -1.616 </z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>gear_front_left</object-name>
  <property>gear/gear[0]/compression-ft</property>
  <factor>0.3</factor>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>gear_front_left</object-name>
  <property>gear/gear[0]/rollspeed-ms</property>
  <factor> 15 </factor>
  <center>
   <x-m> -2.277 </x-m>
   <y-m> -1.000 </y-m>
   <z-m> -1.616 </z-m>
  </center>
  <axis>
   <x>  0.0 </x>
   <y> -1.0 </y>
   <z>  0.0 </z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>gear_front_right</object-name>
  <property>gear/gear[1]/compression-ft</property>
  <factor>0.3</factor>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>gear_front_right</object-name>
  <property>gear/gear[1]/rollspeed-ms</property>
  <factor> 15 </factor>
  <center>
   <x-m> -2.277 </x-m>
   <y-m>  1.000 </y-m>
   <z-m> -1.616 </z-m>
  </center>
  <axis>
   <x>  0.0 </x>
   <y> -1.0 </y>
   <z>  0.0 </z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>gear_rear_left</object-name>
  <property>gear/gear[2]/compression-ft</property>
  <factor>0.3</factor>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>gear_rear_left</object-name>
  <property>gear/gear[2]/rollspeed-ms</property>
  <factor> 15 </factor>
  <center>
   <x-m>  0.242 </x-m>
   <y-m> -1.000 </y-m>
   <z-m> -1.616 </z-m>
  </center>
  <axis>
   <x>  0.0 </x>
   <y> -1.0 </y>
   <z>  0.0 </z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>gear_rear_right</object-name>
  <property>gear/gear[3]/compression-ft</property>
  <factor>0.3</factor>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>gear_rear_right</object-name>
  <property>gear/gear[3]/rollspeed-ms</property>
  <factor>15</factor>
  <center>
   <x-m> 0.242</x-m>
   <y-m> 1.000</y-m>
   <z-m>-1.616</z-m>
  </center>
  <axis>
   <x> 0.0</x>
   <y>-1.0</y>
   <z> 0.0</z>
  </axis>
 </animation>

<!-- TRANSFORMATION CAR-AIRPLANE -->

<animation>
 <type>pick</type>
 <object-name>wing_left</object-name>
 <object-name>wing_right</object-name>
 <object-name>tail</object-name>
 <action>
  <button>0</button>
  <binding>
   <command>nasal</command>
   <script>aerocar.transform.toggle();</script>
  </binding>
 </action>
</animation>

<animation>
 <type>select</type>
 <object-name>supportbeams</object-name>
 <condition>
  <equals>
   <property>controls/doors/transform/position-norm</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>translate</type>
 <object-name>aileron_left</object-name>
